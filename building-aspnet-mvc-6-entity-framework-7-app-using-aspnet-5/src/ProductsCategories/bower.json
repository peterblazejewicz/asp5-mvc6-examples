{
  "name": "ProductsCategories",
  "private": true,
  "dependencies": {
    "bootstrap": "^3.3.6",
    "jquery": "^2.1.4",
    "jquery-validation": "^1.14.0",
    "jquery-validation-unobtrusive": "^3.2.5"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/css/*.**",
        "dist/fonts/*.*",
        "dist/js/bootstrap{.js,.min.js}"
      ]
    },
    "jquery": {
      "main": "dist/*.**"
    },
	"jquery-validation": {
      "main": "dist/jquery.validate{.js,.min.js}"
    },
    "jquery-validation-unobtrusive": {
      "main": "jquery.validate.unobtrusive{.js,.min.js}"
    }
  }
}
